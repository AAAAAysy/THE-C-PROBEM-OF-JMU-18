#include<stdio.h>
int main()
{
	double salary,sum;
	scanf("%lf",&salary);
	if(salary<=1600){
		sum=0;
	}
	else if(salary>1600&&salary<=2500){
		sum=(salary-1600)*0.05;
	}
	else if(salary>2500&&salary<=3500){
		sum=(salary-1600)*0.1;
	}
	else if(salary>3500&&salary<=4500){
		sum=(salary-1600)*0.15;
	}
	else{
		sum=(salary-1600)*0.2;
	}
	printf("%.2lf",sum);
	return 0;
 } 
